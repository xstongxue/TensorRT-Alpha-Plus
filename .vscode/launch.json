    "version": "0.2.0",
    "configurations": [
        {
            "name": "test", // 创建的这个target的名字
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/webapi_yolov8/build/infer_yolov8",
            "args": ["--flagfile=../flagfile.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/webapi_yolov8/build/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "C/C++: g++ build active file",
